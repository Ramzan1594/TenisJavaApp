<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.Insets?>
<?xml-stylesheet href="styleCSS.css" type="text/css"?>

<VBox xmlns:fx="http://javafx.com/fxml" spacing="25">
	<children>
	<GridPane hgap="20" vgap="20">
			<children>
				<Label text="Equipe : " GridPane.columnIndex="1" GridPane.rowIndex="1" />
				<ComboBox fx:id="CBEquipe" visibleRowCount="7" GridPane.columnIndex="2" GridPane.rowIndex="1" />
			</children>
	</GridPane>
		<HBox spacing="15" alignment="CENTER">
			<children>
				<Button fx:id="BSupprimer" prefWidth="80" prefHeight="20" onAction="#BSupprimerEquipe" text="Supprimer" />
				<Button fx:id="BFermer"    prefWidth="80" prefHeight="20" onAction="#BFermer"		   text="Fermer" />
			</children>
		</HBox>
	</children>
</VBox>